<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>username</th>
      <th>email</th>
      <th>id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.id %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
